<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.chart.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox fx:id="commandVBox" alignment="CENTER_LEFT" prefHeight="1000.0" prefWidth="598.0" style="-fx-background-color: #01FD8E;" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ma.fstt.trackingl.DashboardController">
   <children>
      <BarChart fx:id="barChart" prefHeight="387.0" prefWidth="613.0">
        <xAxis>
          <CategoryAxis side="BOTTOM" fx:id="xAxis" />
        </xAxis>
        <yAxis>
          <NumberAxis fx:id="yAxis" side="LEFT" />
        </yAxis>
         <VBox.margin>
            <Insets bottom="20.0" top="20.0" />
         </VBox.margin>
      </BarChart>
      <VBox prefHeight="200.0" prefWidth="100.0">
         <children>
            <Button mnemonicParsing="false" onAction="#switchCommand" style="-fx-background-color: #FFF5D1;" text="Command">
               <VBox.margin>
                  <Insets left="220.0" top="60.0" />
               </VBox.margin>
               <padding>
                  <Insets bottom="20.0" left="30.0" right="30.0" top="20.0" />
               </padding>
                <font>
                    <Font name="System Bold" size="14.0" />
                </font>
                <effect>
                    <DropShadow color="BLACK" offsetX="5" offsetY="5" radius="10" />
                </effect>
            </Button>
         </children>
      </VBox>
   </children>


</VBox>
